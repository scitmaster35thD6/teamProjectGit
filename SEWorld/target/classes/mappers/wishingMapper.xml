<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="global.sesoc.seworld.dao.WishingMapper">

	<insert id="insertOneWishing" parameterType="Wishing">
		INSERT INTO c6_wishing
		(
		memberId
		,exhibitionId
		,createdDate
		)
		VALUES
		(
		#{memberId}
		,#{exhibitionId}
		,SYSDATE
		)
	</insert>

	<delete id="deleteOneWishing" parameterType="Wishing">
		DELETE FROM c6_wishing
		WHERE memberId = #{memberId}
		  AND exhibitionId = #{exhibitionId}
	</delete>

</mapper>