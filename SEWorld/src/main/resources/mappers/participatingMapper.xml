<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="global.sesoc.seworld.dao.ParticipatingMapper">

	<insert id="insertOneParticipating" parameterType="Participating">
		INSERT INTO c6_participating
		(
		memberId
		,exhibitionId
		,createdDate
		)
		VALUES
		(
		#{memberId}
		,#{exhibitionId}
		,SYSDATE
		)
	</insert>

	<delete id="deleteOneParticipating" parameterType="Participating">
		DELETE FROM c6_participating
		WHERE memberId = #{memberId}
		  AND exhibitionId = #{exhibitionId}
	</delete>

</mapper>